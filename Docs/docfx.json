{
  "metadata": [
    {
      "src": [
        {
          "src": "../Src",
          "files": ["**.csproj"]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "GBX.NET Documentation",
      "_appFooter": "© 2022 Petr 'BigBang1112' Pivoňka",
	    "_appLogoPath": "images/logo.png",
	    "_appFaviconPath": "images/favicon.ico"
    },
    "dest": "_site",
    "globalMetadataFiles": ["global.json"],
    "fileMetadataFiles": [],
    "template": [
      "default", "templates/DiscordFX/discordfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}